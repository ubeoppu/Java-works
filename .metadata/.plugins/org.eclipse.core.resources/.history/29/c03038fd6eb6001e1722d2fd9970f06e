package ex01;

public class Con1 {

	public static void main(String[] args) {
		
		double d;
		
		int sum = 100;
		
		d= sum;
		
		System.out.println(d);
		System.out.println(sum);
		
		short s1 = 32000;
		
		int i1 ;
		
		i1 = s1;
		
		System.out.println(s1);
		System.out.println(i1);
		
		int i2 = 32000;
		
		short s2;
		
		double s3 = 32000;
		
		
		s2 = s3;
		
		System.out.println(i2);
		System.out.println(s2);
	}

}
