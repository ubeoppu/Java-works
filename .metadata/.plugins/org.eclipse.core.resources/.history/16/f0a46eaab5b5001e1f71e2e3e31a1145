package ex01;

public class Hello {

	public static void main(String[] args) {
		int math;
		int eng;
		int kor;
		int total;
		int avg;
		
		kor = 70;
		eng = 50;
		math = 30;
		
		total = kor + eng + math;
		avg = total /3;
		
				
		System.out.println("평균 점수:" +avg);
		// TODO Auto-generated method stub

}
}
